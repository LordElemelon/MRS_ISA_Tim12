{
  "name": "room",
  "base": "PersistedModel",
  "idInjection": true,
  "indexes": {
    "numberHotelIndex": {
      "keys": {
        "number": 1,
        "hotelId": -1
      },
      "options": {
        "unique": true
      }
    }
  },
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "number": {
      "type": "number",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "beds": {
      "type": "number",
      "required": true,
      "default": 1
    },
    "active": {
      "type": "boolean",
      "required": true,
      "default": true
    }
  },
  "validations": [],
  "relations": {
    "hotel": {
      "type": "belongsTo",
      "model": "hotel",
      "foreignKey": ""
    },
    "roomPrices": {
      "type": "hasMany",
      "model": "roomPrice",
      "foreignKey": ""
    },
    "hotelDiscounts": {
      "type": "hasMany",
      "model": "hotelDiscount",
      "foreignKey": ""
    },
    "roomReservations": {
      "type": "hasMany",
      "model": "roomReservation",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "hotelAdmin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
